    "def p_m(h,m):\n",
    "    if m>=12:\n",
    "        return 1\n",
    "    if h>=m:\n",
    "        return 0\n",
    "    p=1/6*( p_m(h,m+1)+\n",
    "            p_m(h,m+2)+\n",
    "            p_m(h,m+3)+\n",
    "            p_m(h,m+4)+\n",
    "            p_m(h+5,m)+\n",
    "            p_m(h+6,m)\n",
    "           )\n",
    "    return p\n"
   ]
